{
  "name": "contender/contender",
  "description": "Html Contents Parser & Selector.Like a javascript.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "suzunone",
      "email": "suzunone.eleven@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-dom": "*",
    "ext-libxml": "*",
    "ext-mbstring": "*",
    "illuminate/support":  "~5.1|~6.0|~7.0|~8.0|~9.0|~10.0",
    "symfony/css-selector": "^6.2"

  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "mockery/mockery": "^1.5",
    "fzaninotto/faker": "^1.5",
    "symfony/var-dumper": "^6.2",
    "symfony/debug": "^4.4",
    "symfony/console": "^6.2"
  },
  "autoload": {
    "psr-4": {
      "Contender\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tools\\Contender\\": "tools",
      "Tests\\Contender\\": "tests/Tests"
    }
  },
  "scripts" :{
    "fix": [
      "php ./bin/tool.php doc-comments",
      "php ./bin/tool.php doc-md",
      "php-cs-fixer fix"
    ]
  }
}
